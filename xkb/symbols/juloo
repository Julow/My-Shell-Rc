partial
xkb_symbols "custom" {

	// swap lctrl and lcmd
	key <LWIN> { [ Control_L ] };
	key <LCTL> { [ Super_L ] };
	modifier_map Mod4 { <LCTL> };
	modifier_map Control { <LWIN> };

	// eject = delete
	replace key <I169> { [ Delete ] };

	// caps lock = ctrl
	replace key <CAPS> { [ Control_L, Control_L ] };
	modifier_map Control { <CAPS> };

};
